{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": "3",
        "targets": {
          "chrome": "58",
          "ie": 9
        },
        "debug": true
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "corejs": false
    }],
    "@babel/plugin-transform-regenerator"
  ]
}